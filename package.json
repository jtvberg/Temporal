{
  "name": "Temporal",
  "version": "1.2.0",
  "description": "Transient Note Taking",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron --inspect=7070 .",
    "break": "electron --inspect-brk=7070 .",
    "build": "electron-builder -m",
    "release": "electron-builder -m -p onTagOrDraft"
  },
  "repository": "https://github.com/jtvberg/Temporal",
  "homepage": "https://github.com/jtvberg/Temporal",
  "keywords": [
    "Electron",
    "notes"
  ],
  "author": "jtvberg",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.3.0",
    "electron-builder": "^22.8.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "electron-updater": "^4.3.4",
    "jquery": "^3.5.1"
  },
  "build": {
    "appId": "com.jtvberg.temporal",
    "copyright": "Copyright jtvberg",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "publish": {
      "provider": "github"
    }
  }
}
